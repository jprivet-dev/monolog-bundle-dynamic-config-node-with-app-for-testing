{
    "name": "local/poc-bundle",
    "type": "poc-bundle",
    "description": "Local PocBundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Local\\Bundle\\PocBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Local\\Bundle\\PocBundle\\Tests\\": "tests/"
        }
